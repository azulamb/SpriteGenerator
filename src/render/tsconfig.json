{
	"compilerOptions": {
		"outFile": "../../App/app.js",
		"module": "system",
		"moduleResolution": "node",
		"target": "es6",
		"lib": [
			"dom",
			"es2017"
		],
		"strictNullChecks": true,
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": false,
		"sourceMap": false
	},
	"include": [
		"./**/*.ts"
	]
}